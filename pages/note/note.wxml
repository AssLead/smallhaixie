      </view>
    </view>
</view>

<view class="sec10a">
    <view class="container pt20">
      <view class="f15px xh1-title2">活动地点</view>
      <view class="f12px xh1-small2">
        <view class="p">深圳市福田区益田路</view>
        <map id="map" longitude="114.0920100000" latitude="22.5595700000" scale="18" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" bindmarkertap="markertap"  bindregionchange="regionchange" show-location style="width: 100%; height: 158px;margin: 20px 0"></map>
      </view>
    </view>
</view>

<view class="sec10a">
    <view class="container pt20">
