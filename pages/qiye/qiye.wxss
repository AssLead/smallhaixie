.lsqy-title {
    position: relative;
    margin: 20px 0 30px;
    line-height: 40px;
}
.myqy {
    position: absolute;
    top:0px;
    right:0;
    padding: 2px 10px;
    bottom:0;
    line-height: 35px;
    border:1px solid #484848;
    border-radius: 5px;
}
.f18px {
    font-size: 18px !important;
    font-weight: bold;
}
.lsqy-block {
    border-radius: 0.5em;
    background: #f3f3f3;
    margin-bottom: 20px;
}
.lsqy-img image {
	height: 166px;
    border-radius: 0.5em 0.5em 0 0;
}
.lsqy-txt-img image {
    width: 40px;
    height: 40px;
    float: left;
    border-radius: 50%;
    margin-right: 15px;
}
.lsqy-txt-1 {
    float: left;
}
.f14px {
    font-size: 14px !important;
}
.fwb {
    font-weight: bold;
}
.f12px {
    font-size: 12px !important;
}
.lsqy-txt {
    padding: 0.5em;
    color: #484848;
}
.qiyein {
    width: 250px;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
}